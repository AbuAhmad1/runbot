<?xml version="1.0" encoding="utf-8"?>
  <odoo>
     <record id="branch_form" model="ir.ui.view">
         <field name="name">runbot.branch.form</field>
         <field name="model">runbot.branch</field>
         <field name="inherit_id" ref="runbot.branch_form"/>
         <field name="arch" type="xml">
             <xpath expr="//field[@name='config_id']" position="after">
               <field name="dump_id"/>
             </xpath>
         </field>
     </record>
     <record id="branch_view-tree" model="ir.ui.view">
         <field name="name">runbot.branch.tree</field>
         <field name="model">runbot.branch</field>
         <field name="inherit_id" ref="runbot.branch_view_tree"/>
         <field name="arch" type="xml">
             <xpath expr="//field[@name='sticky']" position="after">
               <field name="dump"/>
             </xpath>
         </field>
     </record>
  </odoo>
</xml>
